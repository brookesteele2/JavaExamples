package com.in28mintues.springboot.learn_jpa_and_hibernate.course;

public class Course {
	private long id;
	private String name;
	private String author;
//constructor
//getters
//toString
	@Override
	public String toString() {
		return "Course [id=" + id + ", name=" + name + ", author=" + author + "]";
	}

	public long getId() {
		return id;
	}

	public String getName() {
		return name;
	}

	public String getAuthor() {
		return author;
	}


}
